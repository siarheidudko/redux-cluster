{
  "name": "redux-cluster",
  "version": "2.0.0",
  "description": "TypeScript cluster module for Redux that synchronizes Redux stores across cluster processes via IPC and TCP sockets",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "npm run build:tests && node dist/tests/integration.test.js",
    "test:visual": "npm run build:tests && node dist/tests/visual.test.js",
    "test:server": "npm run build:tests && node dist/tests/server.test.js",
    "test:client": "npm run build:tests && node dist/tests/client.test.js",
    "test:highload": "npm run build:tests && node dist/tests/highload.test.js",
    "test:error": "npm run build:tests && node dist/tests/error.test.js",
    "test:docker": "docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "build:tests": "tsc -p tsconfig.tests.json",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "example:basic": "node examples/basic.js",
    "example:server": "node examples/server.js",
    "example:client": "node examples/client.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/siarheidudko/redux-cluster.git"
  },
  "keywords": [
    "redux",
    "cluster",
    "ipc",
    "process",
    "typescript",
    "synchronization",
    "distributed",
    "tcp",
    "socket"
  ],
  "author": "Siarhei Dudko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siarheidudko/redux-cluster/issues"
  },
  "homepage": "https://github.com/siarheidudko/redux-cluster#readme",
  "dependencies": {
    "@sergdudko/objectstream": "^3.2.26",
    "protoobject": "^1.1.30",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
